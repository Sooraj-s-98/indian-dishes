{
  "name": "full-stack-ass",
  "version": "0.0.0",
  "scripts": {
    "fe:dev": "vite build --mode development",
    "fe:build": "vite build",
    "api:build": "rm -rf dist/ && copyfiles -u 2 src/api/*.js src/api/**/*.js dist",
    "start": "node dist/index.js",
    "dev": "pm2 start dev.config.js",
    "stag": "npm run api:build && npm run fe:build && pm2 start stag.config.js",
    "prod:api": "npm run api:build && pm2 start prod.config.js",
    "prod:fe": "npm run fe:build",
    "lint:api": "eslint --ext .js src/api",
    "lint:fe": "eslint --ext .js,.react src/fe"
  },
  "dependencies": {
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-slot": "^1.1.0",
    "@shadcn/ui": "^0.0.4",
    "@tailwindcss/forms": "^0.5.9",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "fast-csv": "^5.0.1",
    "lucide-react": "^0.445.0",
    "mysql": "^2.18.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2",
    "shadcn-ui": "^0.9.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "winston": "^3.14.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "copyfiles": "^2.4.1",
    "eslint": "^9.9.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "pm2": "^5.4.2",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.12",
    "vite": "^5.4.1"
  }
}
